/*
programa que recibe una cadena del usuario y verid¡fica su longitud
@author: Hernàndez Sànchez Juan Carlos
@date: 26/09/2024
*/

#include <stdio.h>
#include <string.h>

int main (){
    char str[100];
    printf("Ingresa una cadena\n");
    fgets(str, 100, stdin); 

    if(strlen(str) >-1 ){
        printf("Cade de longitud: %d\n", strlen(str));
    }

    return 0;  
}
