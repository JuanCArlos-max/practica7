/*
programa que recibe una cadena y dice si esta vacia o no
@author: Hernàndez Sànchez Juan Carlos
@date: 26/09/2024
*/

#include <stdio.h>
#include <string.h>

int main (){
    char str[100];
    printf("Ingresa una cadena\n");
    fgets(str, 100, stdin); 

        printf("%s", (strlen(str)==1)?"Cadena vacia": "Cadena no vacia\n");

    return 0;  
}
